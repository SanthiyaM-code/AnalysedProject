<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"/>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="28dp"
        android:layout_toEndOf="@+id/checkBox3"
        android:layout_marginTop="2dp"
        app:cardCornerRadius="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView391"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="26dp"
                    android:layout_toEndOf="@+id/dp_act_app_sel"
                    android:text="TextView"
                    android:textSize="13dp"
                    android:textColor="@color/text_blue"
                    android:fontFamily="@font/nunito_semibold"/>

                <TextView
                    android:id="@+id/textView395"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="26dp"
                    android:layout_toEndOf="@+id/textView391"
                    android:text="TextView"
                    android:textSize="14dp"
                    android:textColor="@color/grey"
                    android:fontFamily="@font/nunito"/>

                <TextView
                    android:id="@+id/textView396"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView391"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="1dp"
                    android:layout_toEndOf="@+id/dp_act_app_sel"
                    android:text="TextView"
                    android:textSize="13dp"
                    android:textColor="@color/grey"
                    android:fontFamily="@font/nunito"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/dp_act_app_sel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="22dp"
                    app:cardCornerRadius="250dp">

                    <ImageView
                        android:id="@+id/imageView57"
                        android:layout_width="44dp"
                        android:layout_height="44dp"
                        tools:srcCompat="@tools:sample/avatars" />
                </androidx.cardview.widget.CardView>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="25dp"
                android:layout_marginTop="19dp"
                android:orientation="vertical">



                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/textView397"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/grey"
                        android:fontFamily="@font/nunito"/>

                    <TextView
                        android:id="@+id/textView398"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@id/textView397"
                        android:text="TextView" />

                    <TextView
                        android:id="@+id/textView399"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/textView397"
                        android:layout_marginTop="12dp"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/grey"
                        android:fontFamily="@font/nunito"/>

                    <TextView
                        android:id="@+id/textView400"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView398"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="12dp"
                        android:layout_toEndOf="@id/textView397"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/text_blue"
                        android:fontFamily="@font/nunito"/>

                    <TextView
                        android:id="@+id/textView401"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/textView399"
                        android:layout_marginTop="12dp"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/grey"
                        android:fontFamily="@font/nunito"/>

                    <TextView
                        android:id="@+id/textView402"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/textView400"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="12dp"
                        android:layout_toEndOf="@id/textView401"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/black"
                        android:fontFamily="@font/nunito"/>

                    <TextView
                        android:id="@+id/textView403"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/textView401"
                        android:layout_marginTop="12dp"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/grey"
                        android:fontFamily="@font/nunito"/>

                    <TextView
                        android:id="@+id/textView404"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/textView402"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="12dp"
                        android:layout_toEndOf="@id/textView403"
                        android:text="TextView"
                        android:textSize="14dp"
                        android:textColor="@color/black"
                        android:fontFamily="@font/nunito"/>
                </RelativeLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/button28"
                    android:layout_width="115dp"
                    android:layout_height="38dp"
                    android:layout_marginTop="21dp"
                    android:text="View Resume"
                    android:textSize="13dp"
                    android:textColor="@color/text_blue"
                    android:fontFamily="@font/nunito_semibold"
                    android:background="@drawable/background_solid_white"
                    android:textAllCaps="false"
                    android:layout_marginBottom="2dp"
                    android:layout_marginStart="2dp"/>
            </LinearLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="21dp"
                android:layout_marginBottom="19dp">

                <ImageView
                    android:id="@+id/imageView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_toEndOf="@+id/textView211"
                    app:srcCompat="@drawable/expand_more" />

                <TextView
                    android:id="@+id/textView211"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/nunito"
                    android:text="Show more"
                    android:textColor="@color/grey"
                    android:textSize="15dp" />
            </RelativeLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</RelativeLayout>