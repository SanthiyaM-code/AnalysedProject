<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="250dp"
        android:layout_marginStart="29dp"
        android:layout_marginBottom="23dp"
        android:id="@+id/dash_interview_dp">

        <ImageView
            android:id="@+id/imageView31"
            android:layout_width="49dp"
            android:layout_height="49dp"
            tools:srcCompat="@tools:sample/avatars" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView334"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_toEndOf="@+id/dash_interview_dp"
        android:text="TextView"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:fontFamily="@font/nunito_semibold"/>

    <TextView
        android:id="@+id/textView335"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView334"
        android:layout_marginStart="27dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/dash_interview_dp"
        android:text="TextView"
        android:textSize="14dp"
        android:textColor="@color/black"
        android:fontFamily="@font/nunito"/>

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="44dp"
        app:srcCompat="@drawable/expand_more" />

    <View
        android:id="@+id/cert_line"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/dash_interview_dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="25dp"
        android:background="@color/light_grey" />
</RelativeLayout>